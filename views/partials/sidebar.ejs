<div id="sidebar">
    <ul>
        <div class="sidebar-title">Contents</div>
        <% pageData.sections.forEach((s, j) => { %>
            <% var sectionLink = `/${pageKey}/${j + 1}/${1}` %>
            <%- include('section', {link: sectionLink, name: s.name}) %>
            <% if (pageData.sIndex === j) { %>
                <% s.subSections.forEach((ss, k) => { %>
                    <% var l = `/${pageKey}/${j+1}/${k+1}` %>
                    <% var sel = (k === pageData.ssIndex); %>
                    <%- include('subsection', {link: l, name: ss.name, done: false, selected: sel, showCheckboxes}) %>
                <% }) %>
            <% } %>
        <% }) %>
    </ul>
</div>
