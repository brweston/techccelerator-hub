<h3><%= subSectionInfo.name %></h3>
<% if (subSectionInfo.videoUrl) { %>
    <video  height="500" controls>
        <source src="<%= subSectionInfo.videoUrl %>" type="video/mp4">
    </video>
<% } %>
<% if (subSectionInfo.questions) { %>
    <%- include('quiz', {questions: subSectionInfo.questions}) %>
<% } %>
<%- include('nav-buttons', {back: subSectionInfo.back, next: subSectionInfo.next}) %>