<ul>
    <% sections.forEach(s => { %>
        <% var sectionLink = "/" + s.key + "/" + 1; %>
        <%- include('section', {link: sectionLink, name: s.name, done: s.done}) %>
        <% if (section === s.key) { %>
            <% s.subSections.forEach((ss, k) => { %>
                <% var l = "/" + s.key + "/" + (k+1); %>
                <% var sel = (k === page - 1); %>
                <%- include('subsection', {link: l, name: ss.name, done: ss.done, selected: sel}) %>
            <% }) %>
        <% } %>
    <% }) %>
</ul>